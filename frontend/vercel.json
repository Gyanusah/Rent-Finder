{
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "https://rent-finder-five.vercel.app/"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "no-cache, no-store, must-revalidate"
        }
      ]
    }
  ]
}